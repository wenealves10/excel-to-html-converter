<!DOCTYPE html>
<html lang="pt-BR">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Generated Table</title>
</head>

<body>
    <table>
        <thead>
            <% header.forEach(col =>{%>
            <th><%= col %></th>
            <% }) %>
        </thead>
        <tbody>
            <% body.forEach(rows =>{%>
            <tr>
                <% rows.forEach(row =>{%>
                <td><%= row %> </td>
                <% }) %>
            </tr>
            <% }) %>
        </tbody>
    </table>
</body>

</html>